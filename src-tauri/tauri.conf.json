{
  "$schema": "https://raw.githubusercontent.com/nicegram/nicegram-tauri/refs/heads/main/crates/tauri-utils/schema.json",
  "productName": "inoX-MIX",
  "version": "0.3.0",
  "identifier": "network.inox.mix",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "title": "inoX-MIX v0.3",
    "windows": [
      {
        "title": "inoX-MIX v0.3",
        "width": 1200,
        "height": 800,
        "minWidth": 600,
        "minHeight": 400,
        "resizable": true,
        "fullscreen": false,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/inox-network/inox-mix/releases/latest/download/latest.json"
      ],
      "pubkey": ""
    }
  }
}
